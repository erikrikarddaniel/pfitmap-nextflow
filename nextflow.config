    docker.enabled = true
    docker.temp = 'auto'
    env {
      JOBLIB_TEMP_FOLDER="/tmp"
        }  
    }
  singularity { singularity.enabled = true }
*/
  gtdb_pfitmap { includeConfig 'conf/gtdb_pfitmap.config' }
  test { includeConfig 'conf/test.config' }
}

