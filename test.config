//Config params for running nextflow test for gtdb workflow

profiles {
  test {
    params.genomes = "testdata/genomes/*.faa.gz"
    params.hmms    = "testdata/profiles/*.hmm"
    params.profiles_hierarchy = "testdata/profiles/hmm_profile_hierarchy.tsv"
    params.DBSOURCE = "NCBI:NR:20191118"
    params.hmm_mincov = "0.9"
    params.results = "results/test_results/"
    } 
}

