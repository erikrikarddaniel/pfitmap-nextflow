//Config params for running a test for nextflow for gtdb workflow 

profiles {
  test {
    params.inputgenomes = "testdata/genomes/*.faa.gz"
    params.hmms    = "testdata/profiles/*.hmm"
    params.profiles_hierarchy = "testdata/profiles/hmm_profile_hierarchy.tsv"
    params.dbsource = "GTDB:04-RS89:20190619"
    params.gtdb_arc_metadata = "testdata/arc122_metadata.tsv"
    params.gtdb_bac_metadata = "testdata/bac120_metadata.tsv"
    params.hmm_mincov = "0.9"
    params.outputdir = "test_results/"
    } 
}
